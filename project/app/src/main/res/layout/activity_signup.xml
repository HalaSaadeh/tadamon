<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/boxInsetLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:padding="@dimen/box_inset_layout_padding"
    tools:context=".SignupActivity"
    tools:deviceIds="wear">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="374dp"
        android:layout_height="325dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="390dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="16dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/emailpasssign"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Signup with email and password" />

        <Button
            android:id="@+id/googlesign"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Signup with google" />

        <Button
            android:id="@+id/facebooksign"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Signup with facebook" />

        <Button
            android:id="@+id/button9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Button" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="46dp"
        android:layout_height="27dp"
        android:layout_marginStart="182dp"
        android:layout_marginTop="162dp"
        android:layout_marginEnd="183dp"
        android:layout_marginBottom="542dp"
        android:text="Signup"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>